struct U512T{
    mut higher: U256,
    mut lower: U256
}

Abstract Contract U512() {
    pub fn bigMul(a: U512T, b: U256) -> U512T {
        let mut h = 0
        let mut l = 0

        if (u256Max!() / b < a.lower) {
            h = (a.higher * b) + 1
            l = u256Max!() % b
        } else {
            h = a.higher * b
            l = a.lower * b
        }
        
        return U512T {
            higher: h,
            lower: l
        }
    }
}