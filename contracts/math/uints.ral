struct U512T{
    mut v: [U256; 2]
}

Abstract Contract U512() {
    pub fn bigAdd(a: U256, b: U256) -> U512T {
        let mut result = [0, 0]

        if (u256Max!() - a > b) {
            return U512T { v: [0, a + b] }
        } else {
            result[0] = 1
            result[1] = u256Max!() - a + b - 1
        }

        return U512T { v: result }
    }
}